{
  "functions": {
    "predeploy": [
      "npm run build"
    ]
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      ".env"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source":"/send",
        "destination":"app"
      }

    ]
  }
}
